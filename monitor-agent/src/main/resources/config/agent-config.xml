<?xml version="1.0" encoding="UTF-8"?>
<config>
    <log level="info" console="true"/>
    <type private="true">
        <includepatterns>
            <includepattern>com/jumei/.*</includepattern>
        </includepatterns>
        <excludepatterns>
            <excludepattern>com/intellij/.*|com/google/.*|com/thoughtworks/.*|ch/qos/.*|com/alibaba/.*|net/.*</excludepattern>
        </excludepatterns>
        <forceincludepatterns>
            <forceincludepattern>org/ass/.*</forceincludepattern>
        </forceincludepatterns>
    </type>
    <method private="true">
        <includepatterns>
            <includepattern>
                <owner>.*</owner>
                <name>.*</name>
                <desc>.*</desc>
            </includepattern>
        </includepatterns>
        <excludepatterns>
            <excludepattern>
                <owner>.*</owner>
                <name>set.*</name>
                <desc>.*</desc>
            </excludepattern>
        </excludepatterns>
    </method>
    <codeinserterbuilders>
        <codeinserterbuilder>org.onelab.monitor.agent.transform.asm.inserter.builder.PicControllerBuilder</codeinserterbuilder>
    </codeinserterbuilders>
</config>