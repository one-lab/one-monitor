<?xml version="1.0" encoding="UTF-8"?>
<config>
    <log level="info" console="false"/>
    <type>
        <!--包含的类名, 必须填写-->
        <includepatterns>
            <includepattern>com/(creditcloud|itiancai)/.*/.*Repository</includepattern>
        </includepatterns>
        <!--在包含的类名排除类名, 可以不写-->
        <excludepatterns>
            <!--<excludepattern>^(net/.*|org/.*|com/intellij/.*|com/google/.*|com/thoughtworks/.*|ch/qos/.*|com/alibaba/.*)$</excludepattern>-->
        </excludepatterns>
    </type>
    <method>
        <includepatterns>
            <includepattern>
                <owner>.*</owner>
                <name>(find|findOne|findAll|findBy|save).*</name>
                <desc>.*</desc>
            </includepattern>
            <includepattern>
                <owner>com/(creditcloud|itiancai)/.*/.*Repository</owner>
                <name>.*</name>
                <desc>.*</desc>
            </includepattern>
        </includepatterns>
