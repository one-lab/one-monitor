<?xml version="1.0" encoding="UTF-8"?>
<config>
    <log level="info" console="true"/>
    <type>
        <includepatterns>
            <!-- org/springframework/web/servlet/DispatcherServlet -->
        </includepatterns>
        <excludepatterns>
            <!--<excludepattern>net/.*|org/.*|com/intellij/.*|com/google/.*|com/thoughtworks/.*|ch/qos/.*|com/alibaba/.*</excludepattern>-->
            <!--<excludepattern>scala/.*|com/twitter/.*|org/springframework/.*|org/activiti/.*|com/fasterxml/.*</excludepattern>-->
            <excludepattern>.*</excludepattern>
        </excludepatterns>
    </type>
    <method>
        <excludepatterns>
            <excludepattern>
                <owner>.*</owner>
                <name>hashCode.*|toString.*|equals.*|getClass.*|notify.*|wait.*|set.*|close.*|flush.*</name>
                <desc>.*</desc>
            </excludepattern>
        </excludepatterns>
    </method>
    <codeinserterbuilders>
        <!--<codeinserterbuilder>org.onelab.monitor.agent.transform.asm.inserter.builder.PicControllerBuilder</codeinserterbuilder>-->
    </codeinserterbuilders>
</config>